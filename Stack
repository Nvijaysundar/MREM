class Student:
    def __init__(self, name, roll, age):  # constructor
        self.name = name
        self.roll = roll
        self.age = age
        self.next = None

class Stack:
    def __init__(self):
            self.head = None
    def push(self):
        name = input("Enter Name: ")
        roll = input("Enter Roll: ")
        age = int(input("Enter Age: "))
        newStudent = Student(name, roll, age)
        if self.head == None:
            self.head = newStudent
        else:
            newStudent.next = self.head
            self.head = newStudent

    def pop(self):
        if self.head == None:
            print("Empty List")
        else:
            print(self.head.name,self.head.age,self.head.roll)
            self.head = self.head.next

s = Stack()
s.push()
s.push()
s.push()
s.pop()
